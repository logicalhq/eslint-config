{
  "name": "@logicalhq/eslint-config-vue",
  "version": "1.0.3",
  "author": "Logical <foss@logical.work>",
  "license": "BSD-2-Clause",
  "description": "The ESLint config we use with Vue at at Logical.",
  "main": "index.js",
  "files": [
    "index.js",
    "LICENSE.md",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logicalhq/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/logicalhq/eslint-config/issues"
  },
  "homepage": "https://github.com/logicalhq/eslint-config#readme",
  "keywords": [
    "eslint-config",
    "vue"
  ],
  "scripts": {
    "fmt": "prettier --write \"./**/*.{js,json,md}\"",
    "fmt:check": "prettier --list-different \"./**/*.{js,json,md}\"",
    "lint": "eslint . --ext .js",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --maxWorkers=2 --reporters='default' --reporters='jest-junit'"
  },
  "dependencies": {
    "@logicalhq/eslint-config-base": "^1.0.3"
  },
  "peerDependencies": {
    "eslint-plugin-vue": "^7.11.0"
  }
}
