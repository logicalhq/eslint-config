{
  "name": "@logicalhq/eslint-config-nestjs",
  "version": "1.0.4",
  "author": "Logical <foss@logical.work>",
  "license": "BSD-2-Clause",
  "description": "The ESLint config we use with NestJS at at Logical.",
  "main": "index.js",
  "files": [
    "index.js",
    "LICENSE.md",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logicalhq/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/logicalhq/eslint-config/issues"
  },
  "homepage": "https://github.com/logicalhq/eslint-config#readme",
  "keywords": [
    "eslint-config",
    "nestjs"
  ],
  "scripts": {
    "fmt": "prettier --write \"./**/*.{js,json,md}\"",
    "fmt:check": "prettier --list-different \"./**/*.{js,json,md}\"",
    "lint": "eslint . --ext .js",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --maxWorkers=2 --reporters='default' --reporters='jest-junit'"
  },
  "dependencies": {
    "@logicalhq/eslint-config-base": "^1.0.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "typescript": "^3.4.3"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "typescript": "^3.4.3"
  },
  "gitHead": "2cd0570b97e55713fd698355312f1b0347e5a228"
}
